    "@angular/platform-browser": "10.1.5",
    "@angular/platform-browser-dynamic": "10.1.5",
    "@angular/router": "10.1.5",
    "bootstrap": "4.5.2",
    "core-js": "3.6.5",
    "jquery": "3.5.1",
    "moment": "2.29.1",
    "moment-range": "4.0.2",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.11.1"
  },
  "devDependencies": {
